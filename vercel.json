{
  "rewrites": [{ "source": "/(.*)", "destination": "/index.html" }],
  "routes": [
    {
      "src": "/script-list",
      "dest": "/index.html",
      "status": 200
    },
    {
      "src": "/receipt",
      "dest": "/index.html",
      "status": 200
    },
    {
      "src": "/meeting-summary",
      "dest": "/index.html",
      "status": 200
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html",
      "status": 200
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        }
      ]
    }
  ]
}
